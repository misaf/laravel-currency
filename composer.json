{
    "name": "misaf/vendra-currency",
    "description": "Multi-currency support system with translatable content and hierarchical currency categories",
    "type": "vendra-module",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filamentphp",
        "currency"
    ],
    "authors": [
        {
            "name": "Ehsan Mahmoodi",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/eloquent-sortable": "^4.5.2",
        "spatie/laravel-sluggable": "^3.7.5"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\VendraCurrency\\": "src/",
            "Misaf\\VendraCurrency\\Tests\\": "tests/",
            "Misaf\\VendraCurrency\\Database\\Factories\\": "database/factories/",
            "Misaf\\VendraCurrency\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\VendraCurrency\\Providers\\CurrencyServiceProvider"
            ]
        }
    }
}
