    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "currency"
    ],
    "authors": [
        {
            "name": "Ehsan Mahmoodi",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
